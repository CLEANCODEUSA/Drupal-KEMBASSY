{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "cboden/ratchet": "^0.4.3",
        "composer/installers": "^1.2",
        "drupal/acquia_connector": "^1.22",
        "drupal/acquia_purge": "^1.1",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^2.3",
        "drupal/advanced_help": "^1.0@beta",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/captcha": "^1.1",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/ember": "1.x-dev",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/field_group_migrate": "^3.1",
        "drupal/filefield_sources": "^1.0@alpha",
        "drupal/honeypot": "^2.0",
        "drupal/image_url_formatter": "^1.0@beta",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/job_scheduler": "^3.1",
        "drupal/libraries": "^3.0@alpha",
        "drupal/list_formatter": "2.x-dev",
        "drupal/menu_attributes": "^1.0@beta",
        "drupal/metatag": "^1.13",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/module_filter": "^3.1",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/node_title_help_text": "^1.2",
        "drupal/node_type_class": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/porterstemmer": "^1.1",
        "drupal/redirect": "^1.6",
        "drupal/rename_admin_paths": "^2.0",
        "drupal/restui": "^1.18",
        "drupal/rules": "^3.0@alpha",
        "drupal/scheduler": "^1.3",
        "drupal/seckit": "^1.2",
        "drupal/video_embed_field": "^2.4",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_ajax_history": "^1.5",
        "drupal/views_block_filter_block": "^1.0",
        "drupal/views_conditional": "^1.0",
        "drupal/views_fieldsets": "^3.3",
        "drupal/webform": "^5.19",
        "drupal/workbench": "^1.3",
        "drupal/workbench_email": "^1.7",
        "drupal/workbench_moderation": "^1.6",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^10.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
